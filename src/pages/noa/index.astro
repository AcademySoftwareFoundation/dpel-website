---
// Layout
import MainLayout from "../../layout/MainLayout.astro";
import BlogPostLayout2 from "../../layout/BlogPostLayout2.astro";

// Markdown
const assetContent = await Astro.glob("../../content/articles/noa.md");
---

<MainLayout>
  <div class="pb-32 container mx-auto">
    {
      assetContent.map(asset => (
      <BlogPostLayout2 
        frontmatter={asset.frontmatter}
      />
    ))}
  </div>
</MainLayout>